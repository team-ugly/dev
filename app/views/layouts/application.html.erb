<% if notice != nil%>
<div class="containerflash">
  <div class="row2">

    <div class="col-md-12">
      <div class="alert alert-top alert-dismissable">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
          ×</button><p class="alert"><%= notice %></p>
      </div>
    </div>
  </div>
</div>
  <% end %>
  <% if alert != nil%>
  <div class="containerflash">
    <div class="row2">
      <div class="col-md-12">

        <div class="alert alert-info-alt alert-dismissable">
          <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
            ×</button><p class="alert"><%= alert %></p></div>

      </div>
    </div>
  </div>
<% end %>

<!-- Fixed navbar -->
<div class="navbar navbar-inverse navbar-fixed-top">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/">UGLY N<i class="fa fa-circle"></i>TE</a>
    </div>
    <div class="navbar-collapse collapse">
      <ul class="nav navbar-nav navbar-right">
        <li class="<%= 'active' if current_page?("/") %>"><a href="/">ホーム</a></li>
        <% if user_signed_in? %><% else %>
          <li class="<%= 'active' if current_page?("/users/sign_in") %>"><a href="/users/sign_in">ログイン</a></li>
          <li class="<%= 'active' if current_page?("/users/sign_up") %>"><a href="/users/sign_up">新規登録</a></li>
        <% end %>
        <% if user_signed_in? %>
          <li class="<%= 'active' if current_page?("/places")||current_page?("/places/new") %>"><a href="/places">農地マップ</a></li>
          <li class="<%= 'active' if current_page?("/users/edit") %>"><a href="/users/edit">プロフィール編集</a></li>
          <li><a href="/users/sign_out">ログアウト</a></li>
        <% end %>
        <li><a data-toggle="modal" data-target="#myModal" href="#myModal"><i class="fa fa-envelope-o"></i></a></li>
      </ul>
    </div><!--/.nav-collapse -->
  </div>
</div>



<%= yield %>


